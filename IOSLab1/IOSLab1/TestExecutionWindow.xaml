<Window x:Class="IOSLab1.TestExecutionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TestExecutionWindow" Height="250" Width="400">
    <Grid>
        <Label x:Name="questionLabel" Content="" HorizontalAlignment="Left" Margin="27,20,0,0" VerticalAlignment="Top"/>
        <StackPanel HorizontalAlignment="Left" Height="92" Margin="10,51,0,0" VerticalAlignment="Top" Width="372">
            <RadioButton Name="a" Height="20" Checked="radioButtonChecked"/>
            <RadioButton Name="b" Height="20"/>
            <RadioButton Name="c" Height="20"/>
            <RadioButton Name="d" Height="20"/>
        </StackPanel>
        <Button x:Name="nextButton" Content="Next" HorizontalAlignment="Left" Margin="161,162,0,0" VerticalAlignment="Top" Width="75" Click="nextButton_Click"/>

    </Grid>
</Window>
